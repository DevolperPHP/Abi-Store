      <li id="dailyMoney" onclick="window.location.href='/dailymoney'"><ion-icon class="icon"
          name="calendar-outline"></ion-icon> <span>Daily transaction</span>
      </li>
      <li id="dailyMoney" onclick="window.location.href='/shop/home'"><ion-icon class="icon"
        name="cart-outline"></ion-icon> <span>Shop</span>
    </li>
    <li id="storage" onclick="window.location.href='/localstore/sell'"><ion-icon class="icon" name="storefront-outline"></ion-icon> <span>Store Cashier</span></li>
      </li>
      <li>
