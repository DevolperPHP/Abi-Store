        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
          <li class="nav-item">
            <a class="nav-link" id="clock" aria-current="page" href="#"></a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">|</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">
              <%= user.name%>
            </a>
          </li>
        </ul>
      </div>
    </div>
  </nav>
  <div class="list bg-light">
    <p class="title">Abi-System</p>
    <ul>
      <li id="Dashboard" onclick="window.location.href='/'"><ion-icon class="icon" name="library-outline"></ion-icon>
